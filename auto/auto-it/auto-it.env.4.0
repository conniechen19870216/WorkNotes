#!/bin/csh
#IP of the Server to run IT, ITHOST - COMPCT, ITHOSTD - Dist
setenv ITHOST 135.252.170.194
setenv ITHOSTD 135.252.181.244

#VNC - VMware will show on this DISPLAY,which started on ITHOST.
