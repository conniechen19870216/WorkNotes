#Dir of the OAM and SDM spa on BUILDHOST
setenv APPDIR "/local/it40/fulltar/"
#Filename of the compact application tar
setenv SDMTOOLTARNAME "8650SDM-4.0.0-ATCAv2-2011-10-22-SDM-TOOLS.tar"
